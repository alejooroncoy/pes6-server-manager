{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run styles && npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "PSM",
    "version": "0.0.7"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "powershell",
            "cmd": "powershell.exe",
            "args": true
          },
          {
            "name": "cmd",
            "cmd": "cmd.exe",
            "args": true
          }
        ]
      },
      "dialog": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$APPCACHE/**/*",
          "$APPCACHE",
          "C:/Windows/System32/drivers/etc/hosts",
          "C:/**/*"
        ]
      },
      "os": {
        "all": true
      },
      "http": {
        "all": true,
        "scope": [
          "http://localhost:5000/**/*",
          "https://psm.up.railway.app/**/*",
          "https://psm-server.onrender.com/**/*",
          "https://psm-server.glitch.me/**/*"
        ]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "psm.cache",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": "FC2A6269F06FDEEF66A4F4C252C11ED5AEC243DB",
        "digestAlgorithm": "sha256",
        "timestampUrl": "http://timestamp.comodoca.com"
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [ 
      "https://psm.up.railway.app/psm/ultimate/updater?version={{current_version}}&platform={{target}}&arch={{arch}}",
      "https://psm-server.glitch.me/psm/ultimate/updater?{{current_version}}&platform={{target}}&arch={{arch}}",
      "https://psm-server.onrender.com/psm/ultimate/updater?{{current_version}}&platform={{target}}&arch={{arch}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQ4RTdBRDE3OTEzQkYyMDkKUldRSjhqdVJGNjNuMkFVN2thcVZqTTBaUHhJTGFTS0NLVkladmhNM0tZUzBkNmdTVDNmME5VMzEK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Pes 6 Server Manager (PSM) ðŸŽ‰âš½",
        "width": 800,
        "maxWidth": 200
      }
    ]
  }
}